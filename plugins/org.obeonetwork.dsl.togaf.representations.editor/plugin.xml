<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.obeonetwork.dsl.togaf.representations.presentation.RepresentationsModelWizardID"
            name="%_UI_RepresentationsModelWizard_label"
            class="org.obeonetwork.dsl.togaf.representations.presentation.RepresentationsModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/RepresentationsModelFile.gif">
         <description>%_UI_RepresentationsModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.obeonetwork.dsl.togaf.representations.presentation.RepresentationsEditorID"
            name="%_UI_RepresentationsEditor_label"
            icon="icons/full/obj16/RepresentationsModelFile.gif"
            extensions="representations"
            class="org.obeonetwork.dsl.togaf.representations.presentation.RepresentationsEditor"
            contributorClass="org.obeonetwork.dsl.togaf.representations.presentation.RepresentationsActionBarContributor">
      </editor>
   </extension>

</plugin>
